class AuthGuard {
    constructor(authService, router) {
        this.authService = authService;
        this.router = router;
    }
    canActivate() {
        if (this.authService.isLoggedIn()) {
            this.authService.refreshToken();
            return true;
        }
        else {
            this.authService.logout();
            this.router.navigate(['login']);
            return false;
        }
    }
}
